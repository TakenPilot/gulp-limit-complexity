{
  "name": "gulp-limit-complexity",
  "version": "0.0.2",
  "description": "Limit complexity in JavaScript projects by failing build if function exceed limits",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha -r blanket -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "config": {
    "blanket": {
      "pattern": "index.js",
      "data-cover-never": "node_modules"
    }
  },
  "author": "Dane Stuckel",
  "license": "ISC",
  "repository": "git@github.com:TakenPilot/gulp-limit-complexity.git",
  "dependencies": {
    "escomplex": "^0.2.3",
    "escomplex-ast-moz": "^0.1.5",
    "esprima": "^1.2.2",
    "gulp-util": "^3.0.1",
    "lodash": "^2.4.1",
    "vinyl-map": "^1.0.1"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^1.9.1",
    "coveralls": "^2.11.2",
    "gulp": "^3.8.7",
    "mocha": "^1.21.4",
    "mocha-lcov-reporter": "0.0.1"
  }
}
